{
  "name": "undefinedstudio/yii2-angular-form",
  "type": "yii2-extension",
  "description": "Yii2 Angular Form",
  "authors": [
    {
      "name": "Gabriele Della Casa Venturelli",
      "email": "hardsetting@gmail.com",
      "homepage": "http://twistedmirrorgames.com"
    },
    {
      "name": "Luca Gatti",
      "email": "luca.horn@gmail.com",
      "homepage": "http://twistedmirrorgames.com"
    }
  ],
  "minimum-stability": "dev",
  "keywords": ["yii2", "angular", "angularjs", "form"],
  "homepage": "https://github.com/undefinedstudio/yii2-angular-form",
  "license": "BSD-3-Clause",
  "require": {
    "yiisoft/yii2": "*",
    "composer-plugin-api": "^1.0",
    "undefinedstudio/yii2-angular": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-bootstrap": "*"
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "scripts": {
    "post-update-cmd": "undefinedstudio\\yii2\\angularform\\TestIgnore::postUpdate",
    "post-package-install": [
      "undefinedstudio\\yii2\\angularform\\TestIgnore::postPackageInstall"
    ]
  },
  "autoload": {
    "psr-4": {
      "undefinedstudio\\yii2\\angularform\\": "",
      "undefinedstudio\\yii2\\angularwidgets\\": "widgets"
    }
  }
}
